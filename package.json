    "build-test": "scripts/build-test.sh"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kevincar/gast.git"
  },
  "keywords": [
    "test",
    "testing",
    "unit-testing",
    "gas",
    "tap",
    "test-anything-protocol"

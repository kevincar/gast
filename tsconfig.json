{
  "compilerOptions": {
    "outFile": "./index.js",
    "target": "es5",
    "moduleResolution": "Node",
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "declaration": true
  },
  "include": [
      "./src/**/*"
  ]
}
